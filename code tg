def send_z(message):
	first_name = message.chat.first_name
	chat_id = message.chat.id
	user_name = message.chat.id
	z = message.text
	admin_id = 2056307524
	app_text = []
	app_name = []
	app_username = []
	app_name.append(first_name)
	app_username.append(user_name)
	app_text.append(z)
	client.send_message(admin_id, f"Ğ„ Ğ·Ğ°Ğ¼Ğ¾Ğ²Ğ»ĞµĞ½Ñ Ğ²Ñ–Ğ´: {app_name}.\nĞ™Ğ¾Ğ³Ğ¾ (Ñ—Ñ—) Ğ°Ğ¹Ğ´Ğ¸: {app_username}\nĞ™Ğ¾Ğ³Ğ¾ Ñ‡Ğ¸ Ñ—Ñ— Ğ¿Ñ€Ğ¾Ñ…Ğ°Ğ½Ğ½Ñ:\n{app_text}")
	
	app_name.clear()
	app_username.clear()
	app_text.clear()
	client.send_message(chat_id, f"ğŸŒˆĞ—Ğ°ÑĞ²ĞºĞ° Ğ²Ñ–Ğ´Ğ¿Ñ€Ğ°Ğ²Ğ»ĞµĞ½Ğ°!\nâ±ï¸ĞĞµ Ğ²Ğ´Ğ¾Ğ²Ğ·Ñ– Ğ· Ğ²Ğ°Ğ¼Ğ¸ Ğ·Ğ²ÑĞ¶ÑƒÑ‚ÑŒÑÑ!")

def send_d(message):
	first_name = message.chat.first_name
	chat_id = message.chat.id
