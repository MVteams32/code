import telebot
from telebot import types

client = telebot.TeleBot("")

@client.message_handler(commands=["start"])
def start(message):
	app_markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
	app_keyboard = types.KeyboardButton("ğŸ’°Ğ—Ğ°Ğ¼Ğ¾Ğ²Ğ¸Ñ‚Ğ¸ Ğ¢ĞµĞ»ĞµĞ³Ñ€Ğ°Ğ¼ Ğ±Ğ¾Ñ‚Ğ°")
	app_keyboard1 = types.KeyboardButton("ğŸª“Ğ¦Ñ–Ğ½Ğ¸ Ğ½Ğ° Ğ±Ğ¾Ñ‚Ñ–Ğ²")
	app_keyboard2 = types.KeyboardButton("ğŸ—ï¸Ğ—Ğ²â€™ÑĞ·Ğ°Ñ‚Ğ¸ÑÑ Ğ· Ñ€Ğ¾Ğ·Ñ€Ğ¾Ğ±Ñ–Ñ‚Ğ½Ğ¸ĞºĞ¾Ğ¼")
	app_keyboard3 = types.KeyboardButton("ğŸ’¸Ğ”Ğ¾Ğ½Ğ°Ñ‚")
	app_markup.add(app_keyboard, app_keyboard1, app_keyboard2, app_keyboard3)
	chat_id = message.chat.id
	first_name = message.chat.first_name
	client.send_message(chat_id, f"ğŸ‘‹ĞŸÑ€Ğ¸Ğ²Ñ–Ñ‚ {first_name} !\nâ­Ğ‘Ğ°Ğ¶Ğ°Ñ”Ñˆ Ğ·Ğ°Ğ¼Ğ¾Ğ²Ğ¸Ñ‚Ğ¸ Ğ¢ĞµĞ»ĞµĞ³Ñ€Ğ°Ğ¼ Ğ±Ğ¾Ñ‚Ğ° Ñ‡Ğ¸ Ğ´Ğ»Ñ Ğ¿Ğ¾Ñ‡Ğ°Ñ‚ĞºÑƒ Ğ²Ğ¿Ñ–Ğ²Ğ·Ğ½Ğ°Ñ‚Ğ¸ ÑĞºĞ° Ğ²Ğ°Ñ€Ñ‚Ñ–ÑÑ‚ÑŒ?",  reply_markup = app_markup)
	print(f'Ğ¥Ñ‚Ğ¾ÑÑŒ Ğ½Ğ°Ğ¶Ğ°Ğ² Ğ½Ğ° ĞºĞ½Ğ¾Ğ¿ĞºÑƒ "start". ĞÑ–Ğº: {first_name}')

